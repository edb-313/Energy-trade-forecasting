df_crude_oil <- read_excel("C:/Users/39329/Documents/GitHub/Energy-trade-forecasting/Data/Total Crude Oil Exports by Destination_EIA.xls", sheet = "Data 1",  skip = 2)
names(df_crude_oil)[-2] <- str_replace_all(names(df_crude_oil)[-2],
c("U.S. Exports to " = "",
" of Crude Oil \\(Thousand Barrels\\)" = ""))
crude_oil <- df_crude_oil %>%
pivot_longer(cols = -Date,
names_to = 'Destination country',
