crude_oil$`Amount of Crude Oil (Thousand Barrels)` <- replace_na(crude_oil$`Amount of Crude Oil (Thousand Barrels)`, 0)
ts_crude_oil <- crude_oil %>%
as_tsibble(
index = Date,
key = `Destination country`
)
